            "document_full_details": {
                "id": 7463030,
                "title": "Fun Basecamp stats",
                "private": false,
                "trashed": false,
                "created_at": "2015-01-04T14:50:00.000-06:00",
                "updated_at": "2015-01-04T14:50:00.000-06:00",
                "creator": {
                    "id": 10031487,
                    "name": "Basecamp",
                    "avatar_url": "http:\/\/dge9rmgqjs8m1.cloudfront.net\/global\/56932044f64da36d76aae07d082859c30010\/avatar.gif?r=3",
                    "fullsize_avatar_url": "https:\/\/dge9rmgqjs8m1.cloudfront.net\/global\/56932044f64da36d76aae07d082859c30010\/original.gif?r=3"
                },
                "content": "Since 2004:<br><br><ul><li>Over 8,000,000 Basecamp projects have been created.<\/li><li>Over 9,000,000 people have Basecamp accounts.<\/li><li>Over 50,000,000 to-dos have been added.<\/li><li>Over 100,000,000 files have been shared.<\/li><li>Over 23 terabytes of data is hosted by Basecamp.<\/li><li><span>96% of our customers say they\u2019d recommend Basecamp to a friend, colleague, or co-worker.<\/span><br><\/li><\/ul>",
                "last_updater": {
                    "id": 10031487,
                    "name": "Basecamp"
                },
